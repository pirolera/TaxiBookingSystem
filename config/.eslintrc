{
  "extends": "eslint:recommended",
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "globals": {
    "before": true,
    "after": true,
    "beforeEach": true,
    "afterEach": true,
    "describe": true,
    "it": true,
    "sinon": true
  },
  "rules": {
    "array-bracket-spacing": 2,
    "arrow-body-style": [
      "error",
      "always"
    ],
    "arrow-parens": 0,
    "arrow-spacing": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "callback-return": 2,
    "camelcase": 0,
    "capitalized-comments": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "comma-spacing": 2,
    "comma-style": 2,
    "complexity": 2,
    "computed-property-spacing": 2,
    "consistent-this": 2,
    "default-case": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "func-call-spacing": 2,
    "func-name-matching": 2,
    "func-names": [
      "error",
      "never"
    ],
    "generator-star-spacing": 2,
    "guard-for-in": 2,
    "global-require": 2,
    "handle-callback-err": 2,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "line-comment-position": 2,
    "lines-around-comment": 2,
    "lines-around-directive": 2,
    "max-depth": 2,
    "max-nested-callbacks": 2,
    "max-params": 0,
    "max-statements-per-line": 2,
    "multiline-ternary": [
      "error",
      "never"
    ],
    "new-cap": [
      "error",
      {
        "capIsNewExceptions": [
          "Router"
        ]
      }
    ],
    "new-parens": 2,
    "newline-after-var": 2,
    "newline-before-return": 2,
    "newline-per-chained-call": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-array-constructor": 2,
    "no-await-in-loop": 2,
    "no-bitwise": 2,
    "no-catch-shadow": 2,
    "no-compare-neg-zero": 2,
    "no-confusing-arrow": 2,
    "no-console": 0,
    "no-continue": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-empty-function": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 0,
    "no-extra-label": 2,
    "no-extra-parens": 2,
    "no-floating-decimal": 2,
    "no-global-assign": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-magic-numbers": [
      "error",
      {
        "ignoreArrayIndexes": true,
        "ignore": [
          0,
          1
        ]
      }
    ],
    "no-mixed-operators": 2,
    "no-mixed-requires": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-assign": 2,
    "no-multiple-empty-lines": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-path-concat": 2,
    "no-plusplus": [
      "error",
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "no-proto": 2,
    "no-restricted-globals": 2,
    "no-restricted-imports": 2,
    "no-restricted-modules": 2,
    "no-restricted-properties": 2,
    "no-restricted-syntax": 2,
    "no-return-assign": 2,
    "no-return-await": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-sync": 2,
    "no-tabs": 2,
    "no-template-curly-in-string": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 0,
    "no-unmodified-loop-condition": 2,
    "no-unneeded-ternary": 2,
    "no-unsafe-negation": 2,
    "no-unused-expressions": 2,
    "no-use-before-define": [
      "error",
      "nofunc"
    ],
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "no-useless-rename": 2,
    "no-useless-return": 2,
    "no-var": 1,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "object-curly-newline": [
      "error",
      "always"
    ],
    "object-curly-spacing": 2,
    "object-property-newline": 2,
    "object-shorthand": 2,
    "one-var": [
      "error",
      "never"
    ],
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "prefer-arrow-callback": [
      "error",
      {
        "allowNamedFunctions": true
      }
    ],
    "prefer-const": 2,
    "prefer-destructuring": 0,
    "prefer-numeric-literals": 2,
    "prefer-promise-reject-errors": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 0,
    "quotes": [
      "error",
      "single"
    ],
    "quote-props": [
      "error",
      "as-needed"
    ],
    "radix": 2,
    "require-await": 2,
    "require-jsdoc": [
      "error",
      {
        "require": {
          "FunctionDeclaration": true,
          "MethodDefinition": true,
          "ClassDeclaration": true,
          "ArrowFunctionExpression": true
        }
      }
    ],
    "rest-spread-spacing": 2,
    "semi": 2,
    "semi-spacing": 2,
    "sort-imports": 2,
    "sort-keys": 2,
    "sort-vars": 2,
    "space-before-blocks": [
      "error",
      "always"
    ],
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "space-in-parens": 2,
    "spaced-comment": [
      "error",
      "always",
      {
        "block": {
          "exceptions": [
            "*"
          ],
          "balanced": true
        }
      }
    ],
    "strict": [
      "error",
      "global"
    ],
    "symbol-description": 2,
    "template-curly-spacing": 2,
    "valid-jsdoc": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "yield-star-spacing": 2,
    "yoda": 2
  }
}
